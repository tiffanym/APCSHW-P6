import java.util.ArrayList;
public class ArrayList{
    ArrayList<Integer> L=new ArrayList<>();
    public void collapseDuplicates(ArrayList<Integer> L){
	for (int i=0;i<L.length-1;i++){
	    if (L[i]==L[i+1]){
		L[i]=null;
		L[i+1]=null;
		i=i+1;
	    }
	}
    }

    //  0,1,2,3,4
    //  5,6,1,5,5
}